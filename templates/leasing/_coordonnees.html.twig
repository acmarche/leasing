{% if data|length > 0 %}
    <h3>Coordonnées</h3>
    <table class="table table-bordered">
        <tr>
            <th>Nom</th>
            <td>{{ data.nom }}</td>
        </tr>
        <tr>
            <th>Prénom</th>
            <td>{{ data.prenom }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ data.email }}</td>
        </tr>
        <tr>
            <th>Service</th>
            <td>{{ data.service }}</td>
        </tr>
    </table>
{% endif %}